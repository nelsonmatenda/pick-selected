{
  "manifest_version": 3,
  "name": "Pick selected",
  "description": "Pick the selected word from content script",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["tabs", "activeTab"],
  "host_permissions": ["<all_urls>"]
}
